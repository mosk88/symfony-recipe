<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href= https://cdn.icon-icons.com/icons2/1151/PNG/512/1486505264-food-fork-kitchen-knife-meanns-restaurant_81404.png>
        {% block stylesheets %}
        {% endblock %}

        {% block javascripts %}
            {% block importmap %}{{ importmap('app') }}{% endblock %}
        {% endblock %}
    </head>
    <body>
        <div id="wrapper" class="flex flex-col min-h-screen">
            <header>
                {{ include ('navbar.html.twig')}}
            </header>

            <main class="grow">
                {% block body %}
                {% endblock %}
            </main>

            {% block footer %}
                {{ include ('footer.html.twig')}}
            {% endblock %}        
        </div>
    </body>
</html>
